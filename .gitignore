# Laravel & Vue.js .gitignore

# General
*.log
*.env
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Dependency directories
/node_modules
/pnp.js
.pnp.js
.pnp.cjs
.pnp.loader.mjs
/vite-error-overlay

# Logs
/logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Composer dependencies
/vendor

# Laravel specific
/public/storage
/storage/*.key
/storage/app/public
/storage/framework/cache/data
/storage/logs
/storage/framework/sessions/*
/storage/framework/views/*
/bootstrap/cache/*.php
/.phpunit.result.cache

# Ignore IDE/editor specific files
.idea/
.vscode/
*.sublime-workspace

# Ignore OS generated files
.DS_Store
Thumbs.db

# Vue.js specific
/dist
/node_modules
/.cache

# Vite
/.vite
/vite.lock
/vite.config.ts

# Testing
/tests/coverage
/tests/*.log
/tests/.DS_Store
/tests/results.json

# Build files
/build
/public/hot
/public/mix-manifest.json
/public/*.map
/public/assets/
/public/js/
/public/css/
/public/img/
/public/fonts/

# Ignoring temporary files or sensitive files
*.lock
*.backup
*.bak
*.swp
*.tmp
*.temp
*.orig

# Ignoring SSH files and sensitive data
*.pem
*.crt
*.key

# npm package lock file
package-lock.json
yarn.lock
.pnp.js
.pnp.loader.mjs

# Laravel Telescope
/telescope/*.sqlite
/telescope/entries.json

# Laravel Horizon
/horizon/*.sqlite

# Ignore cache files generated by IDEs
/.vscode
